{
    "config": {
      "target": "http://localhost:3000",
      "phases": [
        {
          "duration": 60,
          "arrivalRate": 50
        }
      ],
      "payload": {
        "path": "./data.csv",
        "order": "sequence",
        "fields": ["name"]
      }
    },
  
    "scenarios": [
      {
        "engine": "socketio",
        "flow": [
          {
            "emit": {
              "channel": "nickname",
              "data": "{{name}}"
            }
          },
          {
            "emit": {
              "channel": "randomjoin",
              "data": {
                "nickname": "{{name}}",
                "train": "7143"
              }
            }
          },
          {
            "think": 15
          },
          {
            "emit": {
              "channel": "end",
              "data": {
                "msg": "nothing"
              }
            }
          },
          {
            "emit": {
              "channel": "joinFair",
              "data": {
                "roomkey": "7143"
              }
            }
          },
          {
            "emit": {
              "channel": "leaveRoom",
              "data": {
                "roomName": "{{name}}"
              }
            }
          },
          {
            "emit": {
              "channel": "end",
              "data": {
                "msg": "nothing"
              }
            }
          }
        ]
      }
    ]
  }
  